{
  "name": "angular-sailsjs-boilerplate-frontend-angular2",
  "version": "0.0.0",
  "description": "Experimental Angular2 frontend implementation for [angular-sailsjs-boilerplate](https://github.com/tarlepp/angular-sailsjs-boilerplate)",
  "repository": {
    "url": "https://github.com/tarlepp/angular-sailsjs-boilerplate-frontend-angular2"
  },
  "scripts": {
    "build.dev": "gulp build.dev",
    "build.dev.watch": "gulp build.dev.watch",
    "build.prod": "gulp build.prod --env prod",
    "build.test": "gulp build.test",
    "build.test.watch": "gulp build.test.watch",
    "docs": "npm run gulp -- build.docs && npm run gulp -- serve.docs",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "gulp": "gulp",
    "karma": "karma",
    "karma.start": "karma start",
    "lint": "gulp tslint",
    "postinstall": "tsd reinstall --clean && tsd link && tsd rebundle && gulp check.versions && gulp postinstall",
    "reinstall": "rimraf node_modules && npm cache clean && npm install",
    "start": "gulp serve --env dev",
    "serve.dev": "gulp serve --env dev",
    "tasks.list": "gulp --tasks-simple",
    "test": "gulp test",
    "tsd": "tsd",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start"
  },
  "author": "Tarmo Lepp√§nen <tarlepp>",
  "license": "MIT",
